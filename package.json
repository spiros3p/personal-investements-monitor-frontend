{
  "name": "personal-investments-monitor-frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --base-href https://spiros3p.github.io/personal-investments-monitor-frontend/",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "e2e": "playwright test",
