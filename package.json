    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx,html}\" --fix",
    "format": "npx prettier \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\" --write",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.3",
